<section class="page-control page-control-large">
  <div class="col-md mb-3">
    <!-- Published -->
    <% if (page.publishedDate) { %> <% let link = (page.link == "home" ? "" : page.link) %>
    <a href="<%- site.url + link %>" class="link-dark"><h1 class="h2 m-0"><%= page.name %></h1></a>
    <!-- Not Published -->
    <% } else { %>
    <h1 class="h2 m-0"><%= page.name %></h1>
    <% } %>
  </div>
</section>
